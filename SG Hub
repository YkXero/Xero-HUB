		if Id == "RUSH" then			
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "RUSH sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=4458805208",
				Time = 4
			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = true
			_G.sk8 = false
			_G.sk9 = false
			_G.cuss = false
			while wait(0.4) do
				if _G.sk7 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=10716387808"	
				end		
			end	
		end
		if Id == "Speed" then
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "Speed sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=4458805208",
				Time = 4
			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = false
			_G.sk8 = true
			_G.sk9 = false
			_G.cuss = false
			while wait(0.4) do
				if _G.sk8 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=10383043357"
				end
			end
		end		
		if Id == "Rickroll" then
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "Rickroll sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=4458805208",
				Time = 4
			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = false
			_G.sk8 = false
			_G.sk9 = true
			_G.cuss = false
			while wait(0.4) do
				if _G.sk9 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=6403436054"
				end
			end
		end		
	end	
})
VisualSection:AddTextbox({ --CustomSkybox
	Name = "Custom Skybox (rbxassetid | roblox.com/asset link)",
	Default = "",
	TextDisappear = true,
	Callback = function(cus)
		_G.sk1 = false
		_G.sk2 = false
		_G.sk3 = false
		_G.sk4 = false
		_G.sk5 = false
		_G.sk6 = false
		_G.sk7 = false
		_G.sk8 = false
		_G.cuss = true
		for count = 1, 1000000 do
			if 	_G.cuss == true then
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = cus
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = cus
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = cus
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = cus
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = cus
				game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = cus
				wait(0.4)
			end
		end
	end
})	

--Settings Section	

SettingsSection:AddSlider({ --Changes FOV
	Name = "Fov Changer",
	Default = 60,
	Min = 30,
	Max = 120,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.5,
	ValueName = "FOV",
	Callback = function(Value)
		local args = {
			[1] = "Settings FOV",
			[2] = Value
		}
		game:GetService("ReplicatedStorage").Remotes.Data.ChangeSetting:FireServer(unpack(args))
	end    
})	
SettingsSection:AddSlider({ --Changes Power Bar
	Name = "Power Bar Changer",
	Default = 3,
	Min = 0,
	Max = 4,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.02,
	ValueName = "PBs",
	Callback = function(Value)
		local args = {
			[1] = "Settings Power Bar Sensitivity Desktop",
			[2] = Value
		}

		game:GetService("ReplicatedStorage").Remotes.Data.ChangeSetting:FireServer(unpack(args))
	end		
})	
