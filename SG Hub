	Name = "Settings Section"
})
--Paragraphs

InformationTab:AddParagraph("Important", "Do not use this GUI to get advantage of any kind, this script is intended to be for ONLY showcase, if you do not follow this rules you got a big chance of getting banned ingame, apart from we do not support the use of this GUI for get advantage ingame.")
InformationTab:AddParagraph("How to use", "Use C to start, L to stop")
CreditsTab:AddParagraph("Developers", "Xero & Mds")

--Loaded GUI notification
Library:MakeNotification({ --Gui Loading notify
	Name = "GUI Loaded!",
	Content = "Thanks for using X&M SG GUI.",
	Image = "rbxassetid://4483345998",
	Time = 4
})


--MapSpeedrunSection

SpeedrunMapList:AddButton({ --Activate map button
	Name = "Activate maps button",
	Callback = function()
		_G.noti = true
		Library:MakeNotification({ 
			Name = "Clicked successfully",
			Content = "Now you can pick any map you want",
			Image = "http://www.roblox.com/asset/?id=5959696880",
			Time = 4
		})
		local Mouse = game:GetService("Players").LocalPlayer:GetMouse()

		Mouse.KeyDown:connect(function(NameOfYourFunction)

			local KeyToToggleScript = NameOfYourFunction:lower()

			if KeyToToggleScript == "c" then
				_G.cfg = 1
			end
			if KeyToToggleScript == "l" then
				_G.cfg = 2
			end
		end)

		local mt = getrawmetatable(game);
		local old = mt.__namecall
		setreadonly(mt,false)
		mt.__namecall = newcclosure(function(remote,...)
			args = {...}
			method = tostring(getnamecallmethod())
			if method == "FireServer" and tonumber(args[1]) and tostring(remote) ~= "SetSpectating"  then
				if _G.cfg == 1 then
					args = _G.args
					return old(remote,unpack(args))
				end
			elseif _G.cfg == 2 then
				return old(remote,...)
			end
			return old(remote,...)
		end)
		setreadonly(mt,true)
	end    
})
