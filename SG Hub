			Time = 4
		})
		_G.r2 = "true"
		while wait(0) do
			if _G.r2 == "true" then
				for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame["Frame_MessageLogDisplay"].Scroller:GetChildren(Frame)) do
					if v.Name == "Frame" then
						v:Destroy()
					end
				end
			end
		end
	end
