--Visuals Section

VisualSection:AddTextbox({ --FakeTime button
	Name = "FakeTime",
	Default = "Put anything here",
	TextDisappear = true,
	Callback = function(Value)
		Library:MakeNotification({
			Name = "Clicked successfully",
			Content = "FakeTime worked successfully",
			Image = "http://www.roblox.com/asset/?id=4458805208",
			Time = 4
		})
		_G.e2 = true
		for count = 1, 100000 do
			if _G.e2 == true then
				game:GetService("Players").LocalPlayer.PlayerGui.MainGui.MainFrame.Playing.Scored.Time.Text = (Value)
				wait(0.01)
			end
		end
	end
})
VisualSection:AddButton({ --Break FakeTime button
	Name = "Delete fake time",
	Callback = function()
		Library:MakeNotification({
			Name = "Clicked successfully",
			Content = "FakeTime successfully removed",
			Image = "http://www.roblox.com/asset/?id=4458805208",
			Time = 4
		})	
		_G.e2 = false
	end    
})
