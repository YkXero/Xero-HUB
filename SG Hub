--Local Lib/Window
local Library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = Library:MakeWindow({Name = "X&M SG GUI", HidePremium = false, SaveConfig = false	, ConfigFolder = "OrionTest", IntroText = "Xero & Mds SG GUI", IntroIcon = "http://www.roblox.com/asset/?id=3019710362"})

--Local Tabs
local MapList = Window:MakeTab({
	Name = "MapList",
	Icon = "http://www.roblox.com/asset/?id=10893855041",
	PremiumOnly = false
})
local VisualTab = Window:MakeTab({
	Name = "Visuals",
	Icon = "http://www.roblox.com/asset/?id=6523858394",
	PremiumOnly = false
})
local CreditsTab = Window:MakeTab({
	Name = "Credits",
	Icon = "http://www.roblox.com/asset/?id=2795572800",
	PremiumOnly = false
})
local SettingsTab = Window:MakeTab({
	Name = "Settings",
	Icon = "http://www.roblox.com/asset/?id=9753762469",
	PremiumOnly = false
})
local ConvertTab = Window:MakeTab({
	Name = "Convert",
	Icon = "http://www.roblox.com/asset/?id=5267823426",
	PremiumOnly = false
})
local InformationTab = Window:MakeTab({
	Name = "Information",
	Icon = "http://www.roblox.com/asset/?id=4871684504",
	PremiumOnly = false
})
--Sections
local SpeedrunMapList = MapList:AddSection({
	Name = "Speedrun Map List"
})
local VisualSection = VisualTab:AddSection({
	Name = "Visuals Section"
})
local ConvertSection = ConvertTab:AddSection({
	Name = "Convert Section"
})
