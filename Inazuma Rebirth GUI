--Visuals Section

VisualSection:AddButton({  --Dark Gamemode Button
	Name = "Dark Gamemode",
	Callback = function()
		Library:MakeNotification({
			Name = "Clicked successfully",
			Content = "Dark Gamemode Successfully added",
			Image = "http://www.roblox.com/asset/?id=5959696880",
			Time = 4
		})	
		_G.DarkFognoti = true
		_G.Fog = true
		_G.disfog = false
		if _G.Fog == true then
			while wait(.1) do
				game:GetService("Lighting").FogEnd = 20
				game:GetService("Lighting").FogColor = Color3.new(0, 0, 0)	
				if _G.disfog == true then break					
				end		
			end
		end			
	end
})
VisualSection:AddButton({ --Remove DarkFog button
	Name = "Remove Fog",
	Callback = function()
		if _G.DarkFognoti ~= true then
			Library:MakeNotification({ 
				Name = "Error",
				Content = "There isn't any dark fog at the moment",
				Image = "http://www.roblox.com/asset/?id=4458805208",
				Time = 4
			})
		elseif _G.DarkFognoti == true then
			Library:MakeNotification({ 
				Name = "Map chosen",
				Content = "Dark fog removed successfully",
				Image = "http://www.roblox.com/asset/?id=5959696880",
				Time = 4
			})
		end
		_G.sky10 = false
		_G.fog = false
		_G.disfog = true
		while wait(0.1) do
			if _G.disfog == true then
				game:GetService("Lighting").FogEnd = 10000
				if _G.Fog == true then break		
				end
			end
		end
	end
})
