		}
		function posi(hole, x, y, z)
			local pos = game:GetService("Workspace").GolfBallsHighlighted:WaitForChild("BallVisual").Anchor.Primary.Position
			if pos.X < (x + 10) and pos.X > (x - 10) then
				if pos.Y < (y + 2) and pos.Y > (y - 2) then
					if pos.Z < (z + 10) and pos.Z > (z - 10) then
						_G.args = hole
					end
				end
			end
		end

		function posit(hole1, hole2, x, y, z)
			local pos = game:GetService("Workspace").GolfBallsHighlighted:WaitForChild("BallVisual").Anchor.Primary.Position
			wait(0.001)
			if pos.X < (x + 5) and pos.X > (x - 5) then
				if pos.Y < (y + 2) and pos.Y > (y - 2) then
					if pos.Z < (z + 5) and pos.Z > (z - 5) then
						_G.args = hole1
						wait(1.2)
						_G.args = hole2
					end
				end
			end
		end

		function posy(hole1, hole2, hole3, x, y, z)
			local pos = game:GetService("Workspace").GolfBallsHighlighted:WaitForChild("BallVisual").Anchor.Primary.Position
			wait(0.001)
			if pos.X < (x + 5) and pos.X > (x - 5) then
				if pos.Y < (y + 2) and pos.Y > (y - 2) then
					if pos.Z < (z + 5) and pos.Z > (z - 5) then
						_G.args = hole1
						wait(0.4)
						_G.args = hole2
						wait(4)
						_G.args = hole3
					end
				end
			end
		end
		while true do
			for count = 1,10 do
				posi(_G.Arc1, 881.404785, 122.648499, -3490.8728)
				posit(_G.Arc2, _G.Arc21, 585.342, 110.357, -3548.8)
				posi(_G.Arc3,   410.931824, 122.357483, -3490.46436)
				posi(_G.Arc4, 364.564636, 143.357483, -3723.26733)
				posi(_G.Arc5,  644.164429, 112.861359, -3848.30054)
				posy(_G.Arc6, _G.Arc61, _G.Arc62, 530.078369, 122.357483, -3889.16211)
				posi(_G.Arc7,   364.537292, 222.14856, -4093.10547)
				posi(_G.Arc8,   624.117554, 209.910263, -4173.95117)
				posi(_G.Arc9,   774.492554, 209.359497, -4292.99023)
				posit(_G.Arc10, _G.Arc101,  798.992554, 218.941498, -4526.39648)
				posi(_G.Arc11,   369.043152, 213.648544, -4329.58398)
