					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=10465994035"
				end
			end
		end
		if Id == "RUSH" then			
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "RUSH sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=5959696880",
