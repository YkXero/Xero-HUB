--Local Lib/Window
local Library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = Library:MakeWindow({Name = "SG v2", HidePremium = false, SaveConfig = false	, ConfigFolder = "OrionTest", IntroText = "Xero SG GUI v2", IntroIcon = "http://www.roblox.com/asset/?id=3019710362"})

--Local Tabs
local MapList = Window:MakeTab({
	Name = "MapList",
	Icon = "http://www.roblox.com/asset/?id=10893855041",
	PremiumOnly = false
})
local VisualTab = Window:MakeTab({
	Name = "Visuals",
	Icon = "http://www.roblox.com/asset/?id=6523858394",
	PremiumOnly = false
})
local OptimizeTab = Window:MakeTab({
	Name = "Optimize",
	Icon = "http://www.roblox.com/asset/?id=4728059490",
	PremiumOnly = false
})
local SettingsTab = Window:MakeTab({
	Name = "Settings",
	Icon = "http://www.roblox.com/asset/?id=9753762469",
	PremiumOnly = false
})
local InformationTab = Window:MakeTab({
	Name = "Information",
	Icon = "http://www.roblox.com/asset/?id=4871684504",
	PremiumOnly = false
})
--Sections

local SpeedrunMapList = MapList:AddSection({
	Name = "Speedrun Map List"
})
local VisualSection = VisualTab:AddSection({
	Name = "Visuals Section"
})
local SettingsSection = SettingsTab:AddSection({
	Name = "Settings Section"
})
local OptimizeSection = OptimizeTab:AddSection({
	Name = "Optimize Section"
})
--Paragraphs
InformationTab:AddParagraph("How to use", "Use C to start, L to stop")

--Loaded GUI notification and things
Library:MakeNotification({ --Gui Loading notify
	Name = "GUI Loaded!",
	Content = "Thanks for using X&M SG GUI.",
	Image = "rbxassetid://4483345998",
	Time = 4
})




--MapSpeedrunSection

SpeedrunMapList:AddButton({ --Activate map button done
	Name = "Click here ong",
	Callback = function()
		_G.noti = true
		Library:MakeNotification({ 
			Name = "Clicked successfully",
			Content = "Now you can pick any map you want",
			Image = "http://www.roblox.com/asset/?id=5959696880",
			Time = 4
		})
		local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
		Mouse.KeyDown:connect(function(NameOfYourFunction)
			local KeyToToggleScript = NameOfYourFunction:lower()
			if KeyToToggleScript == "c" then
				_G.cfg = 1
			end
			if KeyToToggleScript == "l" then
				_G.cfg = 2
			end
		end)
		local mt = getrawmetatable(game);
		local old = mt.__namecall
		setreadonly(mt,false)
		mt.__namecall = newcclosure(function(remote,...)
			args = {...}
			method = tostring(getnamecallmethod())
			if method == "FireServer" and tonumber(args[1]) and tostring(remote) ~= "SetSpectating"  then
				if _G.cfg == 1 then
					args = _G.args
					return old(remote,unpack(args))
				end
			elseif _G.cfg == 2 then
				return old(remote,...)
			end
			return old(remote,...)
		end)
		setreadonly(mt,true)
	end    
})
