			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = true
			_G.sk8 = false
			_G.sk9 = false
			_G.sk10 = false
			_G.cuss = false
			while wait(0.4) do
				if _G.sk7 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = "http://www.roblox.com/asset/?id=10716387808"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=10716387808"	
				end		
			end	
		end
		if Id == "Speed" then
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "Speed sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=5959696880",
				Time = 4
			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = false
			_G.sk8 = true
			_G.sk9 = false
			_G.sk10 = false
			_G.cuss = false
			while wait(0.4) do
				if _G.sk8 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxRt = "http://www.roblox.com/asset/?id=10383043357"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxUp = "http://www.roblox.com/asset/?id=10383043357"
				end
			end
		end		
		if Id == "Rickroll" then
			Library:MakeNotification({
				Name = "Clicked successfully",
				Content = "Rickroll sky selected successfully",
				Image = "http://www.roblox.com/asset/?id=5959696880",
				Time = 4
			})
			_G.sk1 = false
			_G.sk2 = false
			_G.sk3 = false
			_G.sk4 = false
			_G.sk5 = false
			_G.sk6 = false
			_G.sk7 = false
			_G.sk8 = false
			_G.sk9 = true
			_G.sk10 = false
			_G.cuss = false
			while wait(0.4) do
				if _G.sk9 == true then
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxBk = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxDn = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxFt = "http://www.roblox.com/asset/?id=6403436054"
					game:GetService("Lighting"):WaitForChild("Sky").SkyboxLf = "http://www.roblox.com/asset/?id=6403436054"
